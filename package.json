    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "uglify-js": "^3.10.4"
  },
  "homepage": "https://github.com/it-minds-opensource/ts-array-ext",
  "bugs": {
    "url": "https://github.com/it-minds-opensource/ts-array-ext/issues",
